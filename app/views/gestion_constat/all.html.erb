<h1 class="m-b-40">
  <%= link_to :back do %>
    <i class="fa fa-arrow-left"></i>
  <% end %>
  Liste de tous les constats
</h1>

<div class="m-b-40">

</div>

<div class="row">
  <div class="col-lg-12">
    <div class="table-responsive table--no-card m-b-30">
      <table class="table table-borderless table-striped table-earning">
        <thead>
        <tr>
          <th>#</th>
          <th>Titre</th>
          <th>Description</th>
          <th>Date</th>
          <th colspan="3">&nbsp;</th>
        </tr>
        </thead>
        <tbody>
        <% @constats.each do |alerte| %>
          <%= alerte.inspect %>
        <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>