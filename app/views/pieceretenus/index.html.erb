<p id="notice"><%= notice %></p>

<h1>Pieceretenus</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Detail</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pieceretenus.each do |pieceretenu| %>
      <tr>
        <td><%= pieceretenu.name %></td>
        <td><%= pieceretenu.detail %></td>
        <td><%= link_to 'Show', pieceretenu %></td>
        <td><%= link_to 'Edit', edit_pieceretenu_path(pieceretenu) %></td>
        <td><%= link_to 'Destroy', pieceretenu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pieceretenu', new_pieceretenu_path %>
