
<%= simple_form_for(@typeaccident) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :name %>

    <%= f.input :description %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
    <input type="reset", class="btn btn-danger btn-sm" value="Annuler" />
  </div>
<% end %>
