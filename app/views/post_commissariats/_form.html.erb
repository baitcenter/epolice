
<%= simple_form_for(@post_commissariat) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.association :commissariat %>

    <%= f.association :agent %>

    <%= f.association :grade %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
