
<%= simple_form_for(@structure) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :name %>

    <%= f.input :raison %>

    <%= f.input :adresse %>

    <%= f.input :contactname %>

    <%= f.input :contantprenom %>

    <%= f.input :contactphone %>

    <%= f.input :contactemail %>

    <%= f.association :region %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
