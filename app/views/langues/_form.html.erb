
<%= simple_form_for(@langue) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :name %>

  </div>

  <div class="form-actions">
    <%= f.button :submit, 'ajouter langue' %>
    <input type="reset", class="btn btn-danger btn-sm" value="Annuler" />
  </div>
<% end %>
