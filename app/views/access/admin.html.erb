              var ctx = document.getElementById("widgetChart1");
              if (ctx) {
                  ctx.height = 130;
                  var myChart = new Chart(ctx, {
                      type: 'line',
                      data: {
                          labels: ['January', 'February', 'March', 'April', 'May', 'June', 'July'],
