                              padding: {
                                  left: 0,
                                  right: 0,
                                  top: 0,
                                  bottom: 0
                              }
                          },
                          responsive: true,
                          scales: {
                              xAxes: [{
                                  gridLines: {
                                      color: 'transparent',
                                      zeroLineColor: 'transparent'
                                  },
                                  ticks: {
                                      fontSize: 2,
                                      fontColor: 'transparent'
                                  }
                              }],
                              yAxes: [{
                                  display: false,
                                  ticks: {
                                      display: false,
                                  }
                              }]
                          },
                          title: {
                              display: false,
