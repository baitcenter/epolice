  </div>
  <div class="col-lg-6">
    <h3 class="title-1 m-b-25">Contravention Nationale</h3>
    <div class="table-responsive table--no-card m-b-40">
      <table class="table table-borderless table-striped table-earning">
        <thead>
        <tr>
          <th>date</th>
          <th>Motifs</th>
          <th>Piece retenue</th>
        </tr>
        </thead>
        <tbody>
        <% Convocation.order(created_at: :desc).limit(10).each  do |a| %>
          <tr>
            <td><%= a.created_at %></td>
            <td><%= a.infraction.motif if !a.infraction_id.nil? %> </td>
            <td><%= a.pieceretenu.name if !a.pieceretenu_id.nil? %></td>
          </tr>
        <% end %>
        </tbody>
      </table>
    </div>
    <div class="overview-wrap">
      <h2 class="title-1"></h2>
      <%= link_to "#", class: "btn btn-primary" do %>
        <i class="zmdi zmdi-chart"></i>
        Plus de contraventions
      <% end %>
    </div>
  </div>
