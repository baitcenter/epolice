<div class="row">
  <div class="col-lg-6">
    <h3 class="title-1 m-b-25">Alertes nationale</h3>
    <div class="table-responsive table--no-card m-b-40">
      <table class="table table-borderless table-striped table-earning">
        <thead>
        <tr>
          <th>date</th>
          <th>Description</th>
          <th>name</th>
        </tr>
        </thead>
        <tbody>
        <% Alerte.where(statu_id: Statu.last.id).order(created_at: :desc).limit(10).each  do |a| %>
          <tr>
            <td><%= a.created_at %></td>
            <td><%= a.titre %> </td>
            <td><%= a.agent.name %>&nbsp;<%= a.agent.prenom %></td>
          </tr>
        <% end %>
        </tbody>
      </table>
    </div>
    <div class="overview-wrap">
      <h2 class="title-1"></h2>
