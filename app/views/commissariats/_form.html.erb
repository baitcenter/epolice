<%= simple_form_for(@commissariat) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name, label: "Nom" %>
    <%= f.input :phone, label: "Numéro téléphone liaison" %>
    <%= f.association :ville %>
    <%= f.input :quartier %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>