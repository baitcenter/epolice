
<%= simple_form_for(@postepolice) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :name %>

    <%= f.input :location %>

    <%= f.association :agent %>

    <%= f.association :commissariat %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
